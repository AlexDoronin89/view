<Window x:Class="MVVM.AdminLog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM"
        mc:Ignorable="d"
        Title="AdminLog" MaxHeight="195" MaxWidth="185"  MinHeight="195" MinWidth="185">
    <Grid>
        <StackPanel>
            <TextBlock Text="Login" Margin="5" FontSize="20" />
            <TextBox  Margin="5" />
            <TextBlock Text="Password" Margin="5" FontSize="20" />
            <TextBox Margin="5"/>

            <Button Content="Sign in" FontSize="10" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
