<Window x:Class="MVVM.UserDistributionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM"
        xmlns:vm="clr-namespace:MVVM.VM"
        mc:Ignorable="d"
        Title="UserDistributionWindow" MaxHeight="250" MaxWidth="250"  MinHeight="250" MinWidth="250">
    <Window.DataContext>
        <vm:UserDistributionWindowVIewModel/>
    </Window.DataContext>
    <Grid>
        <StackPanel Margin="10" Orientation="Horizontal">
            <Button Content="Войти как Админ" Margin="5" MinWidth="110" Command="{Binding CreateAdminWindow}" />
            <Button Content="Войти как user" Margin="5" MinWidth="110" Command="{Binding CreatePosterWindow}"/>
        </StackPanel>
    </Grid>
</Window>
